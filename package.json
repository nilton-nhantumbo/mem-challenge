{
  "name": "mem-challenge",
  "version": "1.0.0",
  "description": "Web app that's used to visualize the data that is held in memory by the Java application.",
  "author": "Nilton Nhantumbo",
  "scripts": {
    "build": "webpack --mode production",
    "devbuild": "webpack --mode development",
    "start": "webpack-dev-server --open",
    "test": "jest --detectOpenHandles",
    "watch": "webpack --watch"
  },
  "main": "main.js",
  "dependencies": {
    "@babel/polyfill": "~7.2",
    "core-js": "^3.6.5",
    "regenerator-runtime": "^0.13.5",
    "vue": "2.6.11"
  },
  "devDependencies": {
    "@babel/core": "~7.2",
    "@babel/plugin-proposal-class-properties": "~7.3",
    "@babel/plugin-proposal-decorators": "~7.3",
    "@babel/plugin-proposal-json-strings": "~7.2",
    "@babel/plugin-syntax-dynamic-import": "~7.2",
    "@babel/plugin-syntax-import-meta": "~7.2",
    "@babel/preset-env": "~7.3",
    "@vue/cli-plugin-unit-jest": "^4.4.1",
    "@vue/test-utils": "^1.0.3",
    "axios": "^0.19.2",
    "babel-jest": "^26.0.1",
    "babel-loader": "~8.0",
    "compression-webpack-plugin": "~2.0",
    "cross-env": "~5.2",
    "css-loader": "~0.28",
    "directory-named-webpack-plugin": "^4.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-flowtype": "4.7.0",
    "eslint-plugin-import": "^2.20.2",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "~1.7",
    "html-webpack-plugin": "~3.2",
    "jest": "^26.0.1",
    "jest-vue-preprocessor": "^1.7.1",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "~3.2",
    "prettier": "^2.0.5",
    "source-map-loader": "^0.2.4",
    "vue-jest": "^3.0.5",
    "vue-loader": "~15.6",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "~2.6",
    "webpack": "~4.29",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-hot-middleware": "~2.24",
    "webpack-merge": "~4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.history/"
    ]
  },
  "keywords": [],
  "license": "ISC"
}
